# .env.example

# Токен Telegram бота
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# API ключ для Google Gemini
GEMINI_API_KEY=your_google_gemini_api_key_here

# API ключ для OpenWeatherMap
WEATHER_API_KEY=your_openweathermap_api_key_here

# Ключ для распознавания речи
SPEECH_API_KEY=your_speech_recognition_api_key_here

# Путь к SQLite базе данных (если используется)
DATABASE_URL=sqlite:///path_to_your_database.db

# Язык для перевода (например, для Google Translate API)
TRANSLATOR_API_KEY=your_translator_api_key_here

# Настройки для TTS (Text-to-Speech)
TTS_API_KEY=your_text_to_speech_api_key_here

# Другие секретные ключи и параметры
SECRET_KEY=your_secret_key_here
